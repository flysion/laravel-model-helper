    "license": "MIT",
    "authors": [
        {
            "name": "lixx",
            "email": "sss60@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": ">=5.8.0"
    },
    "autoload": {
        "psr-4": {
            "ModelHelper\\": "src/"
        }
    },
    "require-dev": {
        "laravel/framework": "5.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModelHelper\\ModelHelperServiceProvider"
            ],
            "aliases": {

            }
        }
    }
